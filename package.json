{
  "name": "@builton/core-sdk",
  "version": "2.0.0",
  "description": "Builton JavaScript SDK.",
  "main": "src/main.js",
  "scripts": {
    "codacy": "npm run coverage && cat coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha test/collection --recursive",
    "test": "./node_modules/.bin/mocha test/collection --recursive",
    "lint": "./node_modules/.bin/eslint .",
    "webpack": "./node_modules/.bin/webpack",
    "buildAuto": "nodemon --ignore 'dist/*' --ignore 'example/*' --exec npm run build",
    "build": "npm i && ./node_modules/.bin/mocha test/collection --recursive && ./node_modules/.bin/webpack && echo '------ESLINT------\n' && ./node_modules/.bin/eslint . && echo '------------------\n'",
    "release": "release-it",
    "dry-release": "release-it --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BuiltonDev/javascript-sdk.git"
  },
  "author": "Builton <hello@builton.dev> (https://builton.dev/)",
  "license": "MIT",
  "keywords": [
    "Builton",
    "AI",
    "SDK",
    "SAAS",
    "On-demand"
  ],
  "bugs": {
    "url": "https://github.com/BuiltonDev/javascript-sdk/issues"
  },
  "homepage": "https://github.com/BuiltonDev/javascript-sdk#readme",
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/preset-env": "7.1.0",
    "@babel/register": "7.0.0",
    "babel-loader": "8.0.4",
    "babel-plugin-transform-object-assign": "6.22.0",
    "codacy-coverage": "3.4.0",
    "core-js": "2.5.7",
    "eslint": "5.6.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "mocha-lcov-reporter": "1.3.0",
    "nodemon": "1.18.10",
    "release-it": "7.6.1",
    "superagent-mocker": "0.5.2",
    "uglifyjs-webpack-plugin": "2.0.1",
    "webpack": "4.20.2",
    "webpack-cli": "3.1.2"
  },
  "dependencies": {
    "jwt-decode": "^2.2.0",
    "superagent": "3.8.3"
  },
  "browser": "./dist/main.bundle.js"
}
