{
  "name": "@kvass.ai/core-sdk",
  "version": "1.2.0",
  "description": "KVASS JavaScript SDK.",
  "main": "src/main.js",
  "scripts": {
    "codacy": "npm run coverage && cat coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha test/collection --recursive",
    "test": "./node_modules/.bin/mocha test/collection --recursive",
    "lint": "./node_modules/.bin/eslint .",
    "webpack": "./node_modules/.bin/webpack",
    "buildAuto": "nodemon --ignore 'dist/*' --ignore 'example/*' --exec npm run build",
    "build": "npm i && ./node_modules/.bin/mocha test/collection --recursive && ./node_modules/.bin/webpack && echo '------ESLINT------\n' && ./node_modules/.bin/eslint . && echo '------------------\n'",
    "release": "release-it",
    "dry-release": "release-it --dry-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kvassAI/javascript-sdk.git"
  },
  "author": "KVASS AI <hello@kvass.ai> (https://www.kvass.ai/)",
  "license": "MIT",
  "keywords": [
    "KVASS",
    "AI",
    "SDK",
    "SAAS",
    "On-demand"
  ],
  "bugs": {
    "url": "https://github.com/kvassAI/javascript-sdk/issues"
  },
  "homepage": "https://github.com/kvassAI/javascript-sdk#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "codacy-coverage": "3.0.0",
    "core-js": "2.5.5",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.11.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.7.0",
    "istanbul": "0.4.5",
    "mocha": "5.1.1",
    "mocha-lcov-reporter": "1.3.0",
    "nodemon": "1.17.3",
    "release-it": "7.4.5",
    "superagent-mocker": "0.5.2",
    "uglifyjs-webpack-plugin": "1.2.5",
    "webpack": "4.6.0",
    "webpack-cli": "2.1.3"
  },
  "dependencies": {
    "superagent": "3.8.3"
  },
  "browser": "./dist/main.bundle.js"
}
