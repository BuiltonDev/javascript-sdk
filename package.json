{
  "name": "@shareactor/shareactor-sdk",
  "version": "0.1.3",
  "description": "ShareActor JavaScript SDK.",
  "main": "src/main.js",
  "scripts": {
    "codacy": "npm run coverage && cat coverage/lcov.info | ./node_modules/.bin/codacy-coverage && rm -rf ./coverage",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha test/collection --recursive",
    "test": "./node_modules/.bin/mocha test/collection --recursive",
    "lint": "./node_modules/.bin/eslint .",
    "webpack": "./node_modules/.bin/webpack",
    "buildAuto": "nodemon --ignore 'dist/*' --ignore 'example/*' --exec npm run build",
    "build": "npm i && ./node_modules/.bin/mocha test/collection --recursive && ./node_modules/.bin/webpack && echo '------ESLINT------\n' && ./node_modules/.bin/eslint . && echo '------------------\n'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shareactorIO/javascript-sdk.git"
  },
  "author": "ShareActor <hello@shareactor.io> (http://www.shareactor.io)",
  "license": "MIT",
  "keywords": [
    "ShareActor",
    "SDK",
    "SAAS",
    "On-demand"
  ],
  "bugs": {
    "url": "https://github.com/shareactorIO/javascript-sdk/issues"
  },
  "homepage": "https://github.com/shareactorIO/javascript-sdk#readme",
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-object-assign": "6.22.0",
    "babel-preset-es2015": "6.24.1",
    "babel-register": "6.24.1",
    "codacy-coverage": "2.0.2",
    "core-js": "2.4.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "15.0.1",
    "eslint-plugin-import": "2.3.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-react": "7.0.1",
    "istanbul": "0.4.5",
    "mocha": "3.3.0",
    "mocha-lcov-reporter": "1.3.0",
    "nodemon": "1.11.0",
    "superagent-mocker": "0.5.2",
    "uglifyjs-webpack-plugin": "0.4.6",
    "webpack": "3.0.0"
  },
  "dependencies": {
    "superagent": "3.5.2"
  },
  "browser": "./dist/main.bundle.js"
}
